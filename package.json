{
  "name": "hive-website",
  "version": "0.0.1",
  "description": "",
  "main": "bin/start",
  "scripts": {
    "prebuild": "del dist",
    "build": "NODE_ENV=production webpack",
    "prebuild:dev": "del dist",
    "build:dev": "NODE_ENV=development webpack",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "dev": "nodemon -e js,jsx,json,scss,css -w src -w conf -x \"npm run start:dev\"",
    "fix": "standard --fix",
    "prerelease": "del app.tar.gz",
    "release": "NODE_ENV=production npm test && npm run build && node ./bin/compress",
    "start": "NODE_ENV=production node ./bin/start",
    "start:dev": "NODE_ENV=development npm run build:dev && node ./bin/start",
    "pretest": "del coverage coverage.lcov .nyc_output",
    "test": "standard",
    "posttest": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --recursive --exit 'tst/**/*.spec.js'",
    "version": "npm run release && git add --all",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fnalabs/hive-website.git"
  },
  "bugs": {
    "url": "https://github.com/fnalabs/hive-website/issues"
  },
  "homepage": "https://github.com/fnalabs/hive-website#readme",
  "author": "Adam Eilers",
  "license": "Apache-2.0",
  "dependencies": {
    "history": "~4.7.2",
    "koa": "~2.5.2",
    "koa-compress": "~3.0.0",
    "koa-helmet": "~4.0.0",
    "koa-logger": "~3.2.0",
    "koa-static": "~5.0.0",
    "prop-types": "~15.6.2",
    "react": "~16.5.0",
    "react-dom": "~16.5.0",
    "react-helmet": "~5.2.0",
    "react-router": "~4.3.1",
    "react-router-dom": "~4.3.1"
  },
  "devDependencies": {
    "app-module-path": "~2.2.0",
    "archiver": "~3.0.0",
    "autoprefixer": "~9.1.5",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.3",
    "babel-eslint": "~8.2.3",
    "babel-loader": "~7.1.5",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-istanbul": "~4.1.6",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.26.2",
    "babel-preset-react": "~6.24.1",
    "babel-register": "~6.26.0",
    "bulma": "~0.7.1",
    "chai": "~4.1.2",
    "chai-as-promised": "~7.1.1",
    "codecov": "~3.1.0",
    "css-loader": "~1.0.0",
    "cssnano": "~4.1.0",
    "del-cli": "~1.1.0",
    "dirty-chai": "~2.0.1",
    "enzyme": "~3.6.0",
    "file-loader": "~2.0.0",
    "isomorphic-style-loader": "~4.0.0",
    "mocha": "~5.2.0",
    "node-mocks-http": "~1.7.0",
    "node-sass": "~4.9.3",
    "nodemon": "~1.18.4",
    "nyc": "~13.0.1",
    "postcss-loader": "~3.0.0",
    "proxyquire": "~2.1.0",
    "sass-loader": "~7.1.0",
    "sinon": "~6.2.0",
    "standard": "~12.0.1",
    "supertest": "~3.3.0",
    "webpack": "~4.17.2",
    "webpack-cli": "~3.1.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "watermarks": {
      "lines": [
        80,
        90
      ],
      "functions": [
        80,
        90
      ],
      "branches": [
        80,
        90
      ],
      "statements": [
        80,
        90
      ]
    },
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
